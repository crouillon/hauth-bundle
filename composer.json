{
    "name": "lp-digital/hauth-bundle",
    "description": "Social sign for BackBee v1.* instance",
    "type": "backbee-bundle",
    "license": "GPL-3.0+",
    "keywords": ["backbee", "openid", "facebook", "twitter", "google", "bundle"],
    "authors": [
        {
            "name": "Charles Rouillon",
            "email": "charles.rouillon@lp-digital.fr"
        }
    ],
    "require": {
        "php": ">=5.6",
        "backbee/backbee-php": ">=1.2.1",
        "hybridauth/hybridauth": "^2.8"
    },
    "autoload": {
        "psr-4": {
            "LpDigital\\Bundle\\HAuthBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LpDigital\\Bundle\\HAuthBundle\\Test\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0",
        "mikey179/vfsStream": "~1.4.0",
        "codeclimate/php-test-reporter": "dev-master"
    },    
    "minimum-stability": "stable",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}